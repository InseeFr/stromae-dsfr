{
  "name": "stromae-dsfr",
  "private": true,
  "version": "2.5.0",
  "type": "module",
  "scripts": {
    "prepare": "husky && vite-envs update-types",
    "dev": "vite",
    "check": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "_format": "prettier --ignore-unknown .",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --list-different",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "generate-api-client": "orval --config ./orval.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "cross-env STORYBOOK=true storybook build",
    "prestorybook": "react-dsfr update-icons",
    "prebuild-storybook": "react-dsfr update-icons"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.13.6",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.14.1",
    "@inseefr/lunatic": "^3.11.1",
    "@mui/material": "^5.18.0",
    "@mui/x-date-pickers": "^7.29.1",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-router": "^1.134.12",
    "axios": "^1.13.1",
    "date-fns": "^4.1.0",
    "fast-deep-equal": "^3.1.3",
    "he": "^1.2.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "oidc-spa": "10.0.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.5.3",
    "react-number-format": "^5.4.4",
    "tsafe": "^1.8.12",
    "tss-react": "^4.9.19",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.0.0",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/builder-vite": "^8.6.12",
    "@storybook/channels": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "8.6.15",
    "@storybook/theming": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/he": "^1.2.3",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.2",
    "cross-env": "^10.1.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc-d48603a5-20240813",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.2.0",
    "husky": "^9.1.6",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.0.0",
    "orval": "^7.20.0",
    "prettier": "^3.8.1",
    "storybook": "8.6.17",
    "storybook-dark-mode": "^4.0.2",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.0.0",
    "vite-envs": "^4.6.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^2.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --list-different"
    ],
    "!(*.js|*.jsx|*.ts|*.tsx)": "prettier --write --ignore-unknown --list-different"
  }
}
